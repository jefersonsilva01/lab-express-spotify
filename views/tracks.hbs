<div id="tracks">
  <nav>
    <h2>Title</h2>
    <h2>Listen</h2>
  </nav>
  {{#each tracksSearched}}
    <div class="track">
      <span>{{this.name}}</span>
      <div>
        <audio controls src="{{this.preview_url}}"></audio>
        <a href="{{this.preview_url}}">⬇️</a>
      </div>
    </div>
  {{/each}}
</div>